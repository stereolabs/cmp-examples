{
  "name": "GPS Tacker Sample",
  "description": "An app that starts a ZED and sends GPS telemetries.",
  "release": {
    "name": "1.0.0",
    "description": "Sample that sends GPS telemetries.",
    "type": "docker-compose",
    "default_parameters": {
      "requested": {
        "core": { "disable_app": false },
        "telemetryFreq": 1,
        "$telemetryFreq": {
          "name": "Telemetry frequency",
          "order": 5,
          "group": "Telemetry",
          "if": "recordTelemetry",
          "type": "number",
          "min": 1,
          "max": 3600,
          "step": 1,
          "unit": "seconds",
          "description": "Telemetry frequency defined how often telemetry is generated by the app. Every second by default"
        }
      }
    }
  },
  "permissions": {
    "camera": true
  },
  "auto_install": false,
  "restart_policy": "always"
}
